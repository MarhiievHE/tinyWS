  "devDependencies": {
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "eslint-config-metarhia": "^8.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
